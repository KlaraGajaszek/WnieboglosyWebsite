{
  "name": "wnieboglosy",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "zeplin": "zeplin connect",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "cross-env NODE_ENV=development binode -- @remix-run/dev:remix dev",
    "format": "prettier --write .",
    "generate:css": "npx tailwindcss -i ./styles/tailwind.css -o ./app/styles/tailwind.css",
    "postinstall": "remix setup node",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve build",
    "test": "vitest vitest --ui",
    "typecheck": "tsc -b",
    "validate": "run-p \"test -- --run\" lint typecheck",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@remix-run/react": "^1.3.3",
    "@remix-run/serve": "^1.3.3",
    "@storybook/cli": "^6.4.20",
