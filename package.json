    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@remix-run/dev": "^1.3.3",
    "@remix-run/eslint-config": "^1.3.3",
    "@storybook/addon-actions": "^6.4.20",
